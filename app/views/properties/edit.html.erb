<% content_for :head do %>
  <title>Huntivore | Editing <%= @property.title %></title>
  <meta name="description" content="Currently editing <%= @property.title %>.">
<% end %>

<h1>Editing "<%= link_to "#{@property.title}", @property %>"</h1>
<div class="row">
	<div class="col-md-12">
		<%= render partial: "edit_form" %>
	</div>
</div>

<div id="loading">
  <p>Please hold tight - your listing is being updated!
  <img src="https://s3-us-west-1.amazonaws.com/huntivore/images/assets/ajax-loader.gif" /></p>
</div>